<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mavl.youtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".theTest"
            android:screenOrientation="portrait"/>
        <activity android:name=".SelectTest" />
        <activity android:name=".TestResults" />
        <activity android:name=".EditTestActivity_old" />
        <activity
            android:name=".EditTestActivity"
            android:label="@string/edit_mode"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".StatisticActivity" />
        <activity
            android:name=".fabtest"
            android:label="@string/title_activity_fabtest"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>